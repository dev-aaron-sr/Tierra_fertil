/* Navbar */
#navbar {
    background: transparent;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 10;
  }
  
  /* Carrusel con gradiente en la parte superior */
  .hero {
    position: relative;
  }
  
  .hero::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100px; /* Altura del gradiente */
    background: linear-gradient(to bottom, rgba(255, 255, 255, 1), rgba(255, 255, 255, 0)); /* Gradiente blanco a transparente */
    z-index: 5;
  }
  
  .carousel-inner img {
    display: block;
    width: 100%;
    height: auto;
    object-fit: cover;
  }




