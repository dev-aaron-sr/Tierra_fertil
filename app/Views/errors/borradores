este es de un boton de whatsapp chetado

<a href="javascript:void(0);" class="btn-whatsapp" id="whatsappButton">
  <i class="bi bi-whatsapp" id="whatsappIcon"></i>
</a>

<div id="whatsappPopup" class="popup">
  <div class="popup-content">
    <p class="popup-text">Inicia una conversación</p>
    <a href="https://wa.me/+51947492128" target="_blank" class="popup-btn">
      <i class="bi bi-whatsapp"></i> Tierra Fertil WhatsApp
    </a>
  </div>
</div>

<style>
/* Estilos para el botón de WhatsApp chetado */
.btn-whatsapp {
  position: fixed;
  bottom: 20px;
  right: 20px;
  background-color: #25d366;
  color: white;
  font-size: 24px;
  border-radius: 50%;
  padding: 15px;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
  cursor: pointer;
  z-index: 9999;
  transition: all 0.3s ease;
}

.btn-whatsapp.x {
  background-color: red;
  transform: rotate(180deg);
}

/* Ventana popup del boton, esta va a salir al dar click */
.popup {
  display: none;
  position: fixed;
  bottom: 100px;
  right: 20px;
  background-color: white;
  border-radius: 10px;
  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.15);
  padding: 20px;
  width: 250px;
  z-index: 10000;
}

/* Este es el texto que va a tener el popup */
.popup-text {
  font-size: 16px;
  margin-bottom: 10px;
  color: #333;
  text-align: center;
}

/* El boton que redirige a whatsapp lo puse recatangular
   con bordes un poco redondeados */
.popup-btn {
  display: flex;
  align-items: center;
  background-color: #25d366;
  color: white;
  padding: 10px;
  text-decoration: none;
  border-radius: 5px;
  font-size: 16px;
  text-align: center;
  justify-content: center;
  margin-top: 10px;
}

.popup-btn i {
  margin-right: 10px;
  font-size: 18px;
}

</style>

<script>
const whatsappButton = document.querySelector(".btn-whatsapp");
const whatsappIcon = whatsappButton.querySelector("i");
const whatsappPopup = document.querySelector(".popup");

// Evento del click del boton de whatsapp
whatsappButton.addEventListener("click", function (e) {
  e.preventDefault(); 

  // Si el popup está visible, entoces lo cierro
  if (whatsappPopup.style.display === "block") {
    // Cierro el popup
    whatsappPopup.style.display = "none";
    // Cambio al icono de whatsapp
    whatsappButton.classList.remove("x");
    whatsappIcon.classList.remove("bi-x");
    whatsappIcon.classList.add("bi-whatsapp");
  } 
  // Sino entonces abro el popup
  else {
    // Abro el popup
    whatsappPopup.style.display = "block";
    // Cambio el ícono a "X "
    whatsappButton.classList.add("x");
    whatsappIcon.classList.remove("bi-whatsapp");
    whatsappIcon.classList.add("bi-x");
  }
});


</script>